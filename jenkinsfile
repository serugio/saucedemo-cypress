pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'serch', url: 'https://github.com/serugio/saucedemo-cypress']])
                
            }
        }
        stage('Test') {
            steps {
                sh 'npm run E2ETest'
                
            }
        }
    }
}